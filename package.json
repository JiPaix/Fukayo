{
  "name": "fukayo",
  "description": "Read your favorite manga from your favorite website, keep track of your reading progress and share your reading history with your friends.",
  "author": "some People",
  "homepage": "https://fukayo.com",
  "repository": "github:JiPaix/Fukayo",
  "license": "MIT",
  "version": "0.7.100",
  "private": false,
  "engines": {
    "node": ">=v16.13",
    "npm": ">=8.1"
  },
  "main": "packages/main/dist/index.cjs",
  "scripts": {
    "build": "npm run build:main && npm run build:preload && npm run build:renderer && npm run build:api",
    "prebuild:api": "rimraf ./packages/api/dist",
    "prebuild:main": "rimraf ./packages/main/dist",
    "prebuild:preload": "rimraf ./packages/preload/dist",
    "prebuild:renderer": "rimraf ./packages/renderer/dist",
    "build:api": "cd ./packages/api && vite build",
    "build:main": "cd ./packages/main && vite build",
    "build:preload": "cd ./packages/preload && vite build",
    "build:renderer": "cd ./packages/renderer && vite build",
    "compile": "cross-env MODE=production npm run build && electron-builder build --config .electron-builder.config.js --dir --config.asar=false",
    "test": "npm run test:main && npm run test:preload && npm run test:renderer && npm run test:e2e",
    "test:e2e": "npm run build && vitest run",
    "test:main": "vitest run -r packages/main --passWithNoTests",
    "test:preload": "vitest run -r packages/preload --passWithNoTests",
    "test:renderer": "vitest run -r packages/renderer --passWithNoTests",
    "watch": "npm run build && node scripts/watch.js",
    "lint": "eslint . --ext js,ts,vue",
    "typecheck:api": "tsc --noEmit -p packages/api/tsconfig.json",
    "typecheck:main": "tsc --noEmit -p packages/main/tsconfig.json",
    "typecheck:preload": "tsc --noEmit -p packages/preload/tsconfig.json",
    "typecheck:renderer": "vue-tsc --noEmit -p packages/renderer/tsconfig.json",
    "typecheck": "npm run typecheck:main && npm run typecheck:preload && npm run typecheck:renderer && npm run typecheck:api",
    "release:patch": "node ./scripts/release.js --patch",
    "release:minor": "node ./scripts/release.js --minor",
    "release:major": "node ./scripts/release.js --major",
    "release:patch:dry": "node ./scripts/release.js --patch --dry",
    "release:minor:dry": "node ./scripts/release.js --minor --dry",
    "release:major:dry": "node ./scripts/release.js --major --dry"
  },
  "devDependencies": {
    "@intlify/vite-plugin-vue-i18n": "^6.0.1",
    "@quasar/vite-plugin": "^1.1.1",
    "@types/connect-history-api-fallback": "^1.3.5",
    "@types/express": "^4.17.13",
    "@types/lowdb": "^1.0.11",
    "@types/morgan": "^1.9.3",
    "@types/node-forge": "^1.0.4",
    "@types/user-agents": "^1.0.2",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "@vitejs/plugin-vue": "3.0.3",
    "@vue/test-utils": "2.0.2",
    "@vueuse/core": "^9.1.0",
    "cross-env": "7.0.3",
    "electron": "^20.0.2",
    "electron-builder": "^23.3.3",
    "electron-devtools-installer": "3.2.0",
    "electron-updater": "5.2.1",
    "eslint": "^8.22.0",
    "eslint-plugin-vue": "9.3.0",
    "happy-dom": "6.0.4",
    "nano-staged": "0.8.0",
    "nock": "^13.2.9",
    "playwright": "^1.25.0",
    "rimraf": "^3.0.2",
    "simple-git": "^3.12.0",
    "simple-git-hooks": "2.8.0",
    "terser": "^5.14.2",
    "typed-emitter": "^2.1.0",
    "typescript": "4.7.4",
    "user-agents": "^1.0.1113",
    "vite": "^3.0.7",
    "vitest": "^0.21.1",
    "vue": "^3.2.37",
    "vue-tsc": "^0.40.1"
  },
  "dependencies": {
    "@mdi/font": "^7.0.96",
    "@mdi/js": "^7.0.96",
    "@quasar/extras": "^1.15.1",
    "axios": "^0.27.2",
    "cheerio": "^1.0.0-rc.12",
    "connect-history-api-fallback": "^2.0.0",
    "dayjs": "^1.11.5",
    "express": "^4.18.1",
    "file-type": "^17.1.6",
    "filenamify": "^5.1.1",
    "morgan": "^1.10.0",
    "node-forge": "^1.3.1",
    "pinia": "^2.0.18",
    "puppeteer-cluster": "^0.23.0",
    "puppeteer-extra": "^3.3.4",
    "puppeteer-extra-plugin-adblocker-no-vulnerabilities": "^2.12.3",
    "puppeteer-extra-plugin-stealth": "^2.11.1",
    "quasar": "^2.7.7",
    "semver": "^7.3.7",
    "socket.io": "^4.5.1",
    "socket.io-client": "^4.5.1",
    "systeminformation": "^5.12.5",
    "uuid": "^8.3.2",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.3"
  }
}
